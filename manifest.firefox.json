{
	"manifest_version": 2,
	"default_locale": "en",
	"name":"__MSG_extensionName__",
	"description":"__MSG_extensionDescription__",
	"author": "Ryosuke Ohta",
	"version":"1.2.3",
	"icons": {
		"32": "image/icon.svg"
	},
	"background": {
		"scripts": [
			"etc/DEFAULT_OPTION_LIST.js",
			"etc/PRESET_OPTION_LIST.js",
			"lib/ponyfill.js",
			"lib/indexeddb.js",
			"lib/common.js",
			"background/background.js"
		]
	},

	"options_ui": {
		"page": "option/option.html",
		"open_in_tab": true,
		"browser_style": false
	},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"css": [
				"content/content.css"
			],
			"js": [
				"lib/ponyfill.js",
				"content/content.js"
			]
		}
	],
	"permissions": [
		"activeTab",
		"contextMenus",
		"storage",
		"notifications"
	],
	"applications": {
		"gecko": {
		"id": "{31729e3f-c535-43f8-91a0-706f572fc1c6}",
		"strict_min_version": "60.0"
		}
	}
}
