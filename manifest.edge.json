{
	"manifest_version": 2,
	"default_locale": "en",
	"name":"Search Dictionary Faster",
	"description":"When select a text, link list which contains some dictionary will be displayed. So you can search faster.",
	"author": "Ryosuke Ohta",
	"version":"1.1.12",
	"icons": {
		"32": "image/icon.svg"
	},
	"background": {
		"persistent": true,
		"scripts": [
			"lib/ponyfill.js",
			"etc/config.js",
			"background/background.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"css": [
				"content/content.css"
			],
			"js": [
				"lib/ponyfill.js",
				"content/content.js"
			]
		}
	],
	"permissions": [
		"activeTab",
		"contextMenus",
		"storage",
		"notifications"
	],
	"web_accessible_resources": [
		"image/minus.svg",
		"image/plus.svg"
	]
}
