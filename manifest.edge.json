{
	"manifest_version": 2,
	"default_locale": "en",
	"name":"Search Dictionary Faster",
	"description":"When select a text, link list which contains some dictionary will be displayed. So you can search faster.",
	"author": "Ryosuke Ohta",
	"version":"1.8.1",
	"icons": {
		"128": "image/icon128.png",
		"64": "image/icon64.png",
		"48": "image/icon48.png",
		"16": "image/icon16.png"
	},
	"background": {
		"persistent": true,
		"scripts": [
			"etc/CONST.js",
			"etc/DEFAULT_OPTION_LIST.js",
			"etc/PRESET_OPTION_LIST.js",
			"lib/ponyfill.js",
			"lib/indexeddb.js",
			"lib/share.js",
			"lib/common.js",
			"background/background.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": [
				"etc/CONST.js",
				"lib/ponyfill.js",
				"lib/share.js",
				"content/content.js"
			]
		}
	],
	"browser_action": {
		"default_icon": {
			"128": "image/icon128.png",
			"64": "image/icon64.png",
			"48": "image/icon48.png",
			"16": "image/icon16.png"
		},
		"default_title": "__MSG_extensionName__"
	},
	"permissions": [
		"<all_urls>",
		"activeTab",
		"contextMenus",
		"storage",
		"notifications"
	],
	"web_accessible_resources": [
		"image/*"
	]
}
