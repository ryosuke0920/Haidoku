{
   "manifest_version": 2,
   "default_locale": "ja",
   "name":"__MSG_extensionName__",
   "description":"__MSG_extensionDescription__",
   "version":"1.0.0",
   "background": {
      "scripts": [
         "config.js",
         "background.js"
      ]
   },
   "options_ui": {
      "page": "option.html",
      "browser_style": true,
      "open_in_tab": true
   },
   "permissions": [
      "contextMenus",
      "storage",
      "notifications"
   ]
}
